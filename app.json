{
  "expo": {
    "name": "Adversarius",
    "slug": "Adversarius",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icone.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mayrons2.adversarius"
    },
    "web": {
      "favicon": "./assets/imgs/adaptive-icon.png"
    }
  }
}
